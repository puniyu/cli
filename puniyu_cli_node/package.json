{
  "name": "@puniyu/cli",
  "version": "0.1.2",
  "description": "puniyu cli",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "puniyu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puniyu/cli.git"
  },
  "license": "MIT",
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "puniyu",
    "puniyu-cli"
  ],
  "files": [
    "dist"
  ],
  "napi": {
    "binaryName": "puniyu-cli",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "aarch64-apple-darwin"
    ],
    "dtsHeader": "// powered by puniyu-cli\n"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "bin": {
    "puniyu-cli": "main.js"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release -p puniyu_cli_node --esm -o dist",
    "prepublishOnly": "napi prepublish --no-gh-release",
    "create-npm-dirs": "napi create-npm-dirs"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.2.0",
    "@types/node": "^24.7.1"
  },
  "packageManager": "pnpm@10.24.0"
}